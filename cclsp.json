{
  "servers": [
    {
      "extensions": [
        "js",
        "ts",
        "jsx",
        "tsx"
      ],
      "command": [
        "npx",
        "--",
        "typescript-language-server",
        "--stdio"
      ],
      "rootDir": "."
    },
    {
      "extensions": [
        "go"
      ],
      "command": [
        "gopls"
      ],
      "rootDir": "."
    }
  ]
}